{
  "source": {
      "include": ["server/src", "server/package.json"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
  },

  "plugins": ["plugins/markdown"],

  "opts": {
      "encoding": "utf8",
      "destination": "./",
      "recurse": true,
      "verbose": true,
      "template": "./node_modules/clean-jsdoc-theme",
      "theme_opts": {
          "theme": "dark"
      }
  }
}
